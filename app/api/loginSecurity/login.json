          "sql": {
            "query": "SELECT \n  JSON_OBJECTAGG(\n    name, value\n  ) as settings\nFROM settings s\nWHERE s.name IN('timezone','localization_dateformat','localization_puddle_dateformat','localization_timeformat');",
