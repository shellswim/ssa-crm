            ],
            "wheres": {
              "condition": "AND",
              "rules": [
                {
                  "id": "c.uuid",
                  "field": "c.uuid",
                  "type": "string",
                  "operator": "equal",
                  "value": "{{$_GET.class_uuid}}",
                  "data": {
                    "table": "c",
                    "column": "uuid",
                    "type": "text",
                    "columnObj": {
