                "operator": "equal",
                "type": "expression",
                "name": ":P1",
                "value": "{{$_GET.class_uuid}}"
              },
              {
                "operator": "equal",
                "type": "expression",
                "name": ":P2",
                "value": "{{$_GET.student_uuid}}"
              },
              {
                "operator": "greater_or_equal",
                "type": "expression",
                "name": ":P3",
                "value": "{{$_GET.startdate.formatDate('yyyy-MM-dd')}}"
              },
              {
                "operator": "equal",
                "type": "expression",
                "name": ":P4",
                "value": "{{1}}"
              }
