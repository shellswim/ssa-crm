      {
        "name": "query_class_details",
        "module": "dbconnector",
        "action": "single",
        "options": {
          "connection": "db",
          "sql": {
            "type": "SELECT",
            "columns": [
              {
                "table": "classes",
                "column": "startTimeDisplay"
              },
              {
                "table": "days",
                "column": "dayname"
              },
              {
                "table": "classLevels",
                "column": "name",
                "alias": "levelname"
              },
              {
                "table": "staff",
                "column": "firstName"
              },
              {
                "table": "staff",
                "column": "lastName"
              }
            ],
            "table": {
              "name": "classes"
            },
            "primary": "uuid",
            "joins": [
              {
                "table": "days",
                "column": "*",
                "type": "LEFT",
                "clauses": {
                  "condition": "AND",
                  "rules": [
                    {
                      "table": "days",
                      "column": "id",
                      "operator": "equal",
                      "value": {
                        "table": "classes",
                        "column": "day"
                      },
                      "operation": "="
                    }
                  ]
                },
                "primary": "id"
              },
              {
                "table": "classLevels",
                "column": "*",
                "type": "LEFT",
                "clauses": {
                  "condition": "AND",
                  "rules": [
                    {
                      "table": "classLevels",
                      "column": "uuid",
                      "operator": "equal",
                      "value": {
                        "table": "classes",
                        "column": "classlevel_uuid"
                      },
                      "operation": "="
                    }
                  ]
                },
                "primary": "uuid"
              },
              {
                "table": "staff",
                "column": "*",
                "type": "LEFT",
                "clauses": {
                  "condition": "AND",
                  "rules": [
                    {
                      "table": "staff",
                      "column": "uuid",
                      "operator": "equal",
                      "value": {
                        "table": "classes",
                        "column": "instructor_uuid"
                      },
                      "operation": "="
                    }
                  ]
                },
                "primary": "uuid"
              }
            ],
            "query": "SELECT classes.startTimeDisplay, days.dayname, classLevels.name AS levelname, staff.firstName, staff.lastName\nFROM classes\nLEFT JOIN days ON (days.id = classes.day) LEFT JOIN classLevels ON (classLevels.uuid = classes.classlevel_uuid) LEFT JOIN staff ON (staff.uuid = classes.instructor_uuid)",
            "params": []
          }
        },
        "output": true,
        "meta": [
          {
            "type": "text",
            "name": "startTimeDisplay"
          },
