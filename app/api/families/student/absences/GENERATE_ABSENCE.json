{
  "meta": {
    "$_GET": [
      {
        "type": "text",
        "name": "student_uuid"
      },
      {
        "type": "text",
        "name": "makeup_eligible"
      },
      {
        "type": "text",
        "name": "absence_start"
      },
      {
        "type": "text",
        "name": "absence_end"
      }
    ]
  },
  "exec": {
    "steps": {
      "name": "absences",
      "module": "GENERATE_ABSENCES",
      "action": "absenceRequest",
      "options": {
        "student_uuid": "{{$_GET.student_uuid}}",
        "absence_start": "{{$_GET.absence_start}}",
        "absence_end": "{{$_GET.absence_end}}",
        "makeup_eligible": 1
      },
      "meta": [
        {
          "name": "dates",
          "type": "array"
        }
      ],
      "output": true
    }
  }
}