                    {
                      "name": "current",
                      "type": "number"
                    },
                    {
                      "name": "total",
                      "type": "number"
                    },
                    {
                      "name": "offset",
                      "type": "object",
                      "sub": [
                        {
                          "name": "first",
                          "type": "number"
                        },
                        {
                          "name": "last",
                          "type": "number"
                        },
                        {
                          "name": "next",
                          "type": "number"
                        },
                        {
                          "name": "prev",
                          "type": "number"
                        }
                      ]
                    }
                  ]
                },
                {
                  "name": "limit",
                  "type": "number"
                },
                {
                  "name": "timesarray",
                  "type": "array"
                }
              ],
              "output": true
            }
          }
        }
      }
    ]
  }
