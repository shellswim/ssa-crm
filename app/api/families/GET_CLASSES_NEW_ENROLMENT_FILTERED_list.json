                          "type": "text"
                        },
                        {
                          "name": "starttimedec",
                          "type": "number"
                        },
                        {
                          "name": "student_uuid",
                          "type": "text"
                        },
                        {
                          "name": "studentid",
                          "type": "number"
                        },
                        {
                          "name": "uuid",
                          "type": "text"
                        }
                      ]
                    },
                    {
                      "name": "wed",
                      "type": "text"
                    },
                    {
                      "name": "baseRate",
                      "type": "number"
                    },
                    {
                      "name": "shortName",
                      "type": "text"
                    }
                  ]
                },
                {
                  "name": "limit",
                  "type": "number"
                },
                {
                  "name": "offset",
                  "type": "number"
                },
                {
