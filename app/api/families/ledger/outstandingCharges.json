        "name": "owingChargesBulk",
        "module": "dbupdater",
        "action": "custom",
        "options": {
          "connection": "db",
          "sql": {
            "query": "SELECT *,\n    IFNULL((\n    SELECT SUM(tl.amount)\n    FROM payments_enrolcharges tl LEFT JOIN charges_enrolments pWEC on pWEC.id = tl.enrolchargeid\n    WHERE pWEC.chargeId = fc.id\n    ),0.00) AS totalPaid,\n    IFNULL(fc.total - (\n    SELECT SUM(tl.amount)\n    FROM payments_enrolcharges tl LEFT JOIN charges_enrolments pWEC on pWEC.id = tl.enrolchargeid\n    WHERE pWEC.chargeId = fc.id\n    ),fc.total) AS totalOwing\nFROM charges_family fc\nWHERE\n    IFNULL((\n        SELECT SUM(tl.amount)\n        FROM payments_enrolcharges tl LEFT JOIN charges_enrolments pWEC on pWEC.id = tl.enrolchargeid\n        WHERE pWEC.chargeId = fc.id\n    ),0.00) < fc.total AND fc.family = :P1\nORDER BY fc.total;",
            "params": [
              {
                "name": ":P1",
                "value": "{{$_GET.familyid}}",
                "test": "36"
              }
            ]
          }
        },
        "output": false,
