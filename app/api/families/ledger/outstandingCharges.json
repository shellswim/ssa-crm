              {
                "name": "familyStudents",
                "module": "dbupdater",
                "action": "custom",
                "options": {
                  "connection": "db",
                  "sql": {
                    "query": "SELECT DISTINCT sl.studentid as id, s.firstName, s.lastName, s.family, s.level\nFROM (\n    SELECT\n        ec.studentid\n    FROM charges_enrolments ec\n        LEFT OUTER JOIN\n        payments_enrolcharges tl on tl.enrolchargeid = ec.id\n    WHERE\n        (tl.id IS NULL OR tl.amount < ec.enrolGrandTotal)\n        AND ec.chargeId = :P1\n        AND IFNULL((ec.enrolGrandTotal - (\n               SELECT SUM(tl.amount)\n               FROM payments_enrolcharges tl\n                   JOIN charges_enrolments ec2\n                    ON tl.enrolchargeid = ec.id\n               WHERE ec2.id = ec.id\n               )),ec.enrolGrandTotal) > 0\n    GROUP BY ec.id, ec.studentid, ec.startofweek, ec.enrolGrandTotal, ec.enrolid, ec.classDate\n    ORDER BY ec.studentid, ec.startofweek, ec.enrolid\n         ) AS sl JOIN students s ON sl.studentid = s.id",
                    "params": [
                      {
                        "name": ":P1",
                        "value": "{{id}}",
                        "test": "31"
                      }
                    ]
                  }
                },
                "output": true,
                "meta": [
