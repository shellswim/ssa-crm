                          "sql": {
                            "query": "SELECT\n   ec.id,\n   ec.studentid,\n   ec.startofweek,\n   ec.enrolGrandTotal,\n   ec.enrolid,\n   ec.classDate,\n   IFNULL((ec.enrolGrandTotal - (\n       SELECT SUM(tl.amount)\n       FROM payments_enrolcharges tl\n           JOIN charges_enrolments ce ON tl.enrolchargeid = ce.id\n       WHERE ce.id = ec.id\n       )),ec.enrolGrandTotal) AS remaining,\n   IFNULL((\n       SELECT SUM(tl.amount)\n       FROM payments_enrolcharges tl\n           JOIN charges_enrolments ce ON tl.enrolchargeid = ce.id\n       WHERE ce.id = ec.id\n       ),0.00) AS totalPaid\nFROM charges_enrolments ec\n    LEFT OUTER JOIN\n    payments_enrolcharges tl on tl.enrolchargeid = ec.id\nWHERE\n    (tl.id IS NULL OR tl.amount < ec.enrolGrandTotal)\n    AND ec.chargeId = :P1 AND ec.studentid = :P2\n    AND IFNULL((ec.enrolGrandTotal - (\n           SELECT SUM(tl.amount)\n           FROM payments_enrolcharges tl\n               JOIN charges_enrolments ce \n      \t\t\tON tl.enrolchargeid = ce.id\n           WHERE ce.id = ec.id\n           )),ec.enrolGrandTotal) > 0\nGROUP BY ec.id, ec.studentid, ec.startofweek, ec.enrolGrandTotal, ec.enrolid, ec.classDate\nORDER BY ec.studentid, ec.startofweek, ec.enrolid;",
                            "params": [
