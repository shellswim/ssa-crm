{
  "meta": {
    "$_POST": [
      {
        "type": "number",
        "name": "paymentid"
      },
      {
        "type": "text",
        "name": "familyname"
      },
      {
        "type": "text",
        "name": "paymenttotal"
      },
      {
        "type": "array",
        "name": "record",
        "sub": [
          {
            "type": "text",
            "name": "studentname"
          },
          {
            "type": "text",
            "name": "classname"
          },
          {
            "type": "date",
            "name": "classdate"
          },
          {
            "type": "number",
            "name": "lineremaining"
          },
          {
            "type": "number",
            "name": "linepaid"
          },
          {
            "type": "number",
            "name": "id"
          }
        ]
      },
      {
        "type": "date",
        "name": "paymentdate"
      },
      {
        "type": "text",
        "name": "paymenttype"
      },
      {
        "type": "text",
        "name": "reference"
      },
      {
        "type": "text",
        "name": "notes"
      },
      {
        "type": "text",
        "name": "paymenttitle"
      },
      {
        "type": "text",
        "name": "host"
      }
    ]
  },
  "exec": {
    "steps": [
      {
        "name": "pptr_usr",
        "module": "core",
        "action": "setvalue",
        "options": {
          "key": "pptr_usr",
          "value": "{{$_SESSION.pptr_usr}}"
        },
        "meta": [],
        "outputType": "text",
        "output": true
      },
      {
        "name": "pptr_pss",
        "module": "core",
        "action": "setvalue",
        "options": {
          "key": "pptr_pss",
          "value": "{{$_SESSION.pptr_pss}}"
        },
        "meta": [],
        "outputType": "text",
        "output": true
      },
      {
        "name": "publ",
        "module": "core",
        "action": "setvalue",
        "options": {
          "key": "publ",
          "value": "{{$_SESSION.publ}}"
        },
        "meta": [],
        "outputType": "text",
        "output": true
      },
      {
        "name": "basicauth",
        "module": "core",
        "action": "setvalue",
        "options": {
          "key": "basicauth",
          "value": "{{$_SESSION.basicauth}}"
        },
        "meta": [],
        "outputType": "text",
        "output": true
      },
      {
        "name": "testReceipt",
        "module": "HTMLPDFWrapper",
        "action": "ssaPDFConvert",
        "options": {
          "tempPath": "{{$_POST.host}}/pdf_templates/receipts/tuition_receipt",
          "pageWidth": "",
          "pageHeight": "",
          "papersize": "A4",
          "folderName": "/public/testpdf",
          "fileName": "testpdf",
          "username": "{{pptr_usr}}",
          "password": "{{pptr_pss}}",
          "authheader": "{{basicauth}}"
        },
        "meta": [
          {
            "name": "FileName",
            "type": "text"
          },
          {
            "name": "FilePath",
            "type": "text"
          }
        ],
        "output": true
      }
    ]
  }
}