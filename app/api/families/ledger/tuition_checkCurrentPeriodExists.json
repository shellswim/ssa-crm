{
    "meta": {
        "$_GET": [
            {
                "type": "text",
                "name": "startdate"
            },
            {
                "type": "boolean",
                "name": "chargeahead"
            },
            {
                "type": "text",
                "name": "familyid"
            }
        ]
    },
    "exec": {
        "steps": [
            {
                "name": "chargeahead",
                "module": "tuitionDates_enrolChanges",
                "action": "tuitiondates",
                "options": {
                    "familyid": "{{$_GET.familyid}}",
                    "startdate": "{{$_GET.startdate}}"
                },
                "meta": [
                    {
                        "name": "dates",
                        "type": "array"
                    }
                ],
                "output": true
            },
            "lib/families/ledger/tuition_checkCurrentPeriodExists",
            {
                "name": "chargeDetails",
                "module": "core",
                "action": "setvalue",
                "options": {
                    "value": {
                        "chargeexists": "{{exists}}",
                        "chargeahead": "{{$_GET.chargeahead}}"
                    },
                    "key": "chargeDetails"
                },
                "meta": [],
                "outputType": "text",
                "output": true
            }
        ]
    }
}