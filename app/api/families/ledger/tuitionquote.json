{
  "meta": {
    "$_GET": [
      {
        "type": "text",
        "name": "family_uuid"
      },
      {
        "type": "array",
        "name": "periods"
      }
    ]
  },
  "exec": {
    "steps": [
      {
        "name": "chargearray",
        "module": "core",
        "action": "setvalue",
        "options": {
          "key": "chargearray",
          "value": "{{null}}"
        },
        "meta": [],
        "outputType": "text"
      },
      {
        "name": "periodCharges_repeat",
        "module": "core",
        "action": "repeat",
        "options": {
          "repeat": "{{$_GET.periods}}",
          "outputFields": [],
          "exec": {
            "steps": {
              "name": "chargearray_add",
              "module": "core",
              "action": "setvalue",
              "options": {
                "key": "chargearray",
                "value": "{{(chargearray+familycharges)}},"
              },
              "meta": [],
              "outputType": "text"
            }
          }
        },
        "output": true,
        "meta": [
          {
            "name": "$index",
            "type": "number"
          },
          {
            "name": "$number",
            "type": "number"
          },
          {
            "name": "$name",
            "type": "text"
          },
          {
            "name": "$value",
            "type": "object"
          },
          {
            "name": "familycharges",
            "type": "text",
            "sub": [
              {
                "name": "calc_tuition",
                "type": "array",
                "sub": [
                  {
                    "name": "chargefor",
                    "type": "text"
                  },
                  {
                    "name": "monthtocharge",
                    "type": "date"
                  },
                  {
                    "name": "monthlychargeexists",
                    "type": "object",
                    "sub": [
                      {
                        "name": "exists",
                        "type": "boolean"
                      },
                      {
                        "name": "charge_uuid",
                        "type": "text"
                      },
                      {
                        "name": "status",
                        "type": "number"
                      },
                      {
                        "name": "created",
                        "type": "date"
                      }
                    ]
                  },
                  {
                    "name": "enrolments",
                    "type": "array",
                    "sub": [
                      {
                        "name": "uuid",
                        "type": "text"
                      },
                      {
                        "name": "id",
                        "type": "number"
                      },
                      {
                        "name": "enrolmentType",
                        "type": "number"
                      },
                      {
                        "name": "dropDate",
                        "type": "date"
                      },
                      {
                        "name": "isValid",
                        "type": "number"
                      },
                      {
                        "name": "student",
                        "type": "number"
                      },
                      {
                        "name": "student_uuid",
                        "type": "text"
                      },
                      {
                        "name": "startDate",
                        "type": "date"
                      },
                      {
                        "name": "classId",
                        "type": "number"
                      },
                      {
                        "name": "class_uuid",
                        "type": "text"
                      },
                      {
                        "name": "dropReason",
                        "type": "date"
                      },
                      {
                        "name": "isTransferIn",
                        "type": "number"
                      },
                      {
                        "name": "isTransferOut",
                        "type": "number"
                      },
                      {
                        "name": "transferTo",
                        "type": "text"
                      },
                      {
                        "name": "transferFrom",
                        "type": "text"
                      },
                      {
                        "name": "transferToStart",
                        "type": "date"
                      },
                      {
                        "name": "priceOverride",
                        "type": "number"
                      },
                      {
                        "name": "priceOverrideValue",
                        "type": "number"
                      },
                      {
                        "name": "priceOverrideType",
                        "type": "number"
                      },
                      {
                        "name": "debuggerstatus",
                        "type": "text"
                      },
                      {
                        "name": "classday",
                        "type": "number"
                      },
                      {
                        "name": "classType",
                        "type": "number"
                      },
                      {
                        "name": "classtype_uuid",
                        "type": "text"
                      },
                      {
                        "name": "shortName",
                        "type": "text"
                      },
                      {
                        "name": "firstName",
                        "type": "text"
                      },
                      {
                        "name": "lastName",
                        "type": "text"
                      },
                      {
                        "name": "instructor_uuid",
                        "type": "text"
                      },
                      {
                        "name": "classlevel_uuid",
                        "type": "text"
                      },
                      {
                        "name": "pricing",
                        "type": "object",
                        "sub": [
                          {
                            "name": "enNetRate",
                            "type": "number"
                          },
                          {
                            "name": "endisc",
                            "type": "number"
                          },
                          {
                            "name": "endiscrate",
                            "type": "text"
                          },
                          {
                            "name": "endiscdescription",
                            "type": "text"
                          },
                          {
                            "name": "baseRate",
                            "type": "number"
                          },
                          {
                            "name": "enrolmentOrder",
                            "type": "number"
                          },
                          {
                            "name": "familydiscounttotal",
                            "type": "number"
                          },
                          {
                            "name": "familydiscountrate",
                            "type": "text"
                          },
                          {
                            "name": "familydiscountdesc",
                            "type": "text"
                          },
                          {
                            "name": "enrolsubtotal",
                            "type": "number"
                          }
                        ]
                      },
                      {
                        "name": "classdate",
                        "type": "date"
                      },
                      {
                        "name": "startofweek",
                        "type": "date"
                      }
                    ]
                  },
                  {
                    "name": "totals",
                    "type": "object",
                    "sub": [
                      {
                        "name": "baseRate",
                        "type": "number"
                      },
                      {
                        "name": "enrolsnet",
                        "type": "number"
                      },
                      {
                        "name": "enrolsdisctotal",
                        "type": "number"
                      },
                      {
                        "name": "familydisctotal",
                        "type": "number"
                      },
                      {
                        "name": "disctotal",
                        "type": "number"
                      },
                      {
                        "name": "familygrandtotal",
                        "type": "number"
                      }
                    ]
                  }
                ]
              }
            ]
          }
        ],
        "outputType": "array"
      },
      {
        "name": "chargearray_final",
        "module": "core",
        "action": "setvalue",
        "options": {
          "value": "{{chargearray.replace(',', ' ').trim().split(' ')}}"
        },
        "meta": [],
        "outputType": "text",
        "output": false
      }
    ]
  }
}