      },
      {
        "name": "query_enrolments",
        "module": "dbconnector",
        "action": "select",
        "options": {
          "connection": "db",
          "sql": {
            "type": "SELECT",
            "columns": [
              {
                "table": "s",
                "column": "firstName"
              },
              {
                "table": "s",
                "column": "lastName"
              },
              {
                "table": "classes",
