          },
          {
            "type": "text",
            "name": "instructor_uuid"
          },
          {
            "type": "number",
            "name": "enrolmentType"
          },
          {
            "type": "date",
            "name": "startDate"
          },
          {
            "type": "date",
            "name": "dropDate"
          },
          {
            "type": "number",
            "name": "deleted_time"
          }
        ],
        "outputType": "array"
      }
