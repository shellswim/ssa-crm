{
  "meta": {
    "options": {
      "linkedFile": "/views/familydetail.ejs",
      "linkedForm": "createGuardianForm"
    },
    "$_GET": [
      {
        "type": "text",
        "name": "familyid"
      }
    ],
    "$_POST": [
      {
        "type": "text",
        "fieldName": "firstName",
        "name": "firstName"
      },
      {
        "type": "text",
        "fieldName": "lastName",
        "name": "lastName"
      },
      {
        "type": "text",
        "fieldName": "phone",
        "name": "phone"
      },
      {
        "type": "text",
        "fieldName": "email",
        "name": "email"
      },
      {
        "type": "text",
        "fieldName": "relationship",
        "name": "relationship"
      },
      {
        "type": "text",
        "name": "familyid"
      }
    ]
  },
  "exec": {
    "steps": {
      "name": "insert",
      "module": "dbupdater",
      "action": "insert",
      "options": {
        "connection": "db",
        "sql": {
          "type": "insert",
          "values": [
            {
              "table": "guardians",
              "column": "firstName",
              "type": "text",
              "value": "{{$_POST.firstName}}"
            },
            {
              "table": "guardians",
              "column": "lastName",
              "type": "text",
              "value": "{{$_POST.lastName}}"
            },
            {
              "table": "guardians",
              "column": "phone",
              "type": "text",
              "value": "{{$_POST.phone}}"
            },
            {
              "table": "guardians",
              "column": "email",
              "type": "text",
              "value": "{{$_POST.email}}"
            },
            {
              "table": "guardians",
              "column": "family",
              "type": "text",
              "value": "{{$_POST.familyid}}"
            },
            {
              "table": "guardians",
              "column": "relationship",
              "type": "text",
              "value": "{{$_POST.relationship}}"
            }
          ],
          "table": "guardians",
          "returning": "id",
          "query": "INSERT INTO guardians\n(firstName, lastName, phone, email, family, relationship) VALUES (:P1 /* {{$_POST.firstName}} */, :P2 /* {{$_POST.lastName}} */, :P3 /* {{$_POST.phone}} */, :P4 /* {{$_POST.email}} */, :P5 /* {{$_POST.familyid}} */, :P6 /* {{$_POST.relationship}} */)",
          "params": [
            {
              "name": ":P1",
              "type": "expression",
              "value": "{{$_POST.firstName}}"
            },
            {
              "name": ":P2",
              "type": "expression",
              "value": "{{$_POST.lastName}}"
            },
            {
              "name": ":P3",
              "type": "expression",
              "value": "{{$_POST.phone}}"
            },
            {
              "name": ":P4",
              "type": "expression",
              "value": "{{$_POST.email}}"
            },
            {
              "name": ":P5",
              "type": "expression",
              "value": "{{$_POST.familyid}}"
            },
            {
              "name": ":P6",
              "type": "expression",
              "value": "{{$_POST.relationship}}"
            }
          ]
        }
      },
      "meta": [
        {
          "name": "identity",
          "type": "text"
        },
        {
          "name": "affected",
          "type": "number"
        }
      ],
      "output": true
    }
  }
}