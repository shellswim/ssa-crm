{
  "meta": {
    "$_PARAM": [
      {
        "type": "text",
        "name": "family_uuid"
      },
      {
        "type": "text",
        "name": "enrolment_startdate"
      }
    ]
  },
  "exec": {
    "steps": {
      "name": "generate",
      "module": "tuitioncalc_monthgen",
      "action": "tuitioncalc_monthgen",
      "options": {
        "family_uuid": "{{$_PARAM.family_uuid}}",
        "startdate": "{{$_PARAM.enrolment_startdate}}"
      },
      "meta": [
        {
          "name": "chargearray",
          "type": "array",
          "sub": [
            {
              "name": "newcharge",
              "type": "boolean"
            },
            {
              "name": "montharray",
              "type": "array"
            }
          ]
        }
      ],
      "output": true
    }
  }
}