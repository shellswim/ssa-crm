{
  "bootstrap4": {
    "basic": {
      "headings-font-family": "Comfortaa",
      "primary": "#FF304F",
      "font-family-base": "Nunito",
      "secondary": "#CACACA",
      "font-size-base": "0.9",
      "enable-responsive-font-sizes": true,
      "border-radius": ".6rem",
      "enable-validation-icons": false,
      "info": "#0F81C7",
      "light": "#ececec",
      "success": "#015668",
      "danger": "#06648C",
      "warning": "#0DE2EA",
      "dark": "#222222"
    },
    "advanced": {
      "gray-300": "#e0e0e0"
    }
  },
  "bootstrap5": {
    "basic": {
      "headings-font-family": "Rubik:400",
      "primary": "#8b20f0",
      "secondary": "#146ab5",
      "enable-responsive-font-sizes": true,
      "info": "#5FE0FF",
      "light": "#efefef",
      "success": "#75bf08",
      "danger": "#FF0053",
      "warning": "#ffaf09",
      "dark": "#142040",
      "link-color": "",
      "theme": "Poypull",
      "font-family-base": "Rubik:400",
      "font-family-monospace": "Roboto Mono:400",
