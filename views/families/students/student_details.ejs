<!-- Wappler include head-page="layouts/layout2" is="dmx-app" id="student_details" appConnect="local" fontawesome_5="cdn" bootstrap5="local" jquery_35="cdn" bootstrap_icons="cdn" components="{dmxBootstrap5Navigation:{},dmxFormatter:{},dmxBootbox:{},dmxBootstrap5TableGenerator:{},dmxMoment:{},dmxBootstrap5Modal:{},dmxDataTraversal:{},dmxSummernote:{}}" moment_2="cdn" -->
<dmx-serverconnect id="getStudentDetail" url="/api/students/getStudentDetail" dmx-param:studentid="query.studentid" dmx-param:systime="localTimeDays.datetime.formatDate('yyyy-MM-dd')" dmx-param:enrol_lifetime="sel_classTimeline.value" dmx-param:note_lifetime="sel_notesTimeline.value"></dmx-serverconnect>
<dmx-serverconnect id="familyStudentList" url="/api/families/studentList" dmx-param:familyid="query.familyid"></dmx-serverconnect>
<dmx-serverconnect id="note_sc_closeOpen" noload></dmx-serverconnect>
<div class="modal" id="view_edit_notesModal" is="dmx-bs5-modal" tabindex="-1" dmx-on:hidden-bs-modal="dd_studentNote.form_editNote.reset();getStudentDetail.load({studentid: query.studentid})">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header bg-secondary text-light">
                <h5 class="modal-title" dmx-on:click=""><b>Student Note: </b>{{dd_studentNote.data.subject}}&nbsp;</h5>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">

                <dmx-data-detail id="dd_studentNote" dmx-bind:data="getStudentDetail.data.notes" key="id">
                    <form is="dmx-serverconnect-form" id="form_editNote" method="post" action="/api/students/notes/updateNote" dmx-generator="bootstrap5" dmx-form-type="inline" dmx-on:success="notifies1.success('Note successfully updated.');view_edit_notesModal.hide()">
                        <input type="hidden" class="form-control" id="inp_id" name="id" aria-describedby="inp_id_help" placeholder="Enter Id" dmx-bind:value="dd_studentNote.data.id">
                        <div class="row mb-3">
                            <div class="col">
                                <label for="inp_subject" class="col-form-label">Subject</label>
                                <input type="text" class="form-control" id="inp_subject" name="subject" aria-describedby="inp_subject_help" placeholder="Enter Subject" dmx-bind:value="dd_studentNote.data.subject">
                            </div>
                        </div>
                        <div class="row mb-3">
                            <div class="col">
                                <label for="inp_priority" class="col-form-label">Priority</label>
                                <select id="sel_priority" class="form-select" name="priority" dmx-bind:value="dd_studentNote.data.priority">
                                    <option value="1">Lowest</option>
                                    <option value="2">Low</option>
                                    <option value="3">Medium</option>
                                    <option value="4">High</option>
                                    <option value="5">Critical</option>
                                </select>
                            </div>
                        </div>
                        <div class="row mb-3">
                            <div class="col">
                                <label for="inp_content" class="col-form-label">Content</label>
                                <textarea dmx-bind:value="dd_studentNote.data.content" id="inp_content" name="content" is="dmx-summernote" dmx-bind:toolbar="[['style',['style']],['font',['bold','underline','clear']],['fontname',['fontname']],['color',['color']],['para',['ul','ol','paragraph']],['table',['table']],['view',['fullscreen','codeview','help']]]" height="300" disable-drop="true"></textarea>
                            </div>
                        </div>
                    </form>
                </dmx-data-detail>
            </div>
            <div class="modal-footer d-flex justify-content-between">

                <button type="button" class="btn btn-outline-danger" data-bs-dismiss="modal"><i class="bi-x-circle me-2"></i>Close
                </button>
                <button type="button" class="btn btn-primary" dmx-on:click="dd_studentNote.form_editNote.submit()"><i class="bi-save2 me-2"></i>Save changes
                </button>
            </div>
        </div>
    </div>
</div>
<dmx-datetime id="localTimeDays" interval="days"></dmx-datetime>
<meta name="ac:route" content="/families/familydetail/:familyid/studentdetail/:studentid">
