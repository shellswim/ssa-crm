<body is="dmx-app" id="layoutMain">
    <div class="modal" id="modal1" is="dmx-bs4-modal" tabindex="-1" role="dialog">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title">Create User</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <form is="dmx-serverconnect-form" id="form1" method="post" action="api/loginSecurity/registerUser" dmx-generator="bootstrap4" dmx-form-type="horizontal" dmx-on:success="browser1.alert('User Created');modal1.hide()"
                        dmx-on:error="browser1.alert('Something went wrong! ')">
                        <div class="form-group row">
                            <label for="inp_username" class="col-sm-2 col-form-label">Username</label>
                            <div class="col-sm-10">
                                <input type="text" class="form-control" id="inp_username" name="username" aria-describedby="inp_username_help" placeholder="Enter Username" required="">
                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="inp_password" class="col-sm-2 col-form-label">Password</label>
                            <div class="col-sm-10">
                                <input type="password" class="form-control" id="inp_password" name="password" aria-describedby="inp_password_help" placeholder="Enter Password" required="" minlength="8"
                                    data-msg-minlength="Please enter at least 8 characters.">
                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="inp_firstName" class="col-sm-2 col-form-label">First Name</label>
                            <div class="col-sm-10">
                                <input type="text" class="form-control" id="inp_firstName" name="firstName" aria-describedby="inp_password_help" placeholder="First Name" required="">
                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="inp_lastName" class="col-sm-2 col-form-label">Last Name</label>
                            <div class="col-sm-10">
                                <input type="text" class="form-control" id="inp_lastName" name="lastName" aria-describedby="inp_password_help" placeholder="Last Name" required="">
                            </div>
                        </div>
                        <div class="form-group row">
                            <div class="col-sm-12 align-self-center d-flex justify-content-end">
                                <button type="submit" class="btn btn-primary">Save</button>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
    <div is="dmx-browser" id="browser1"></div>
    <dmx-preloader id="preloader1" color="#4EDEDB" spinner="chasingDots"></dmx-preloader>







    <main class="w-100 flex-1">
        <div class="container wappler-block pt-3 pb-3">
            <div class="row">
                <div class="col-12 col-xl-4 offset-xl-4 text-center p-5 col-md-8 offset-md-2 bg-secondary rounded">
                    <img class="wappler-type-user mb-4" width="240" src="/assets/images/logo-11.png">
                    <form id="loginForm" method="post" is="dmx-serverconnect-form" action="api/loginSecurity/login" dmx-on:success="browser1.goto('/families')" dmx-on:unauthorized="browser1.alert('Error: Username or Password is incorrect.')"
                        class="mb-1">
                        <div class="form-group text-left text-light">
                            <label for="username" class="col-form-label">Username</label>
                            <input type="text" class="form-control" id="username" name="username" placeholder="Username">
                        </div>
                        <div class="form-group text-left text-light">
                            <label for="password" class="col-form-label">Password</label>
                            <input type="password" class="form-control" id="password" name="password" placeholder="Password">
                        </div>
                        <div class="form-row">
                            <div class="col d-flex justify-content-between"><button class="btn btn-light shadow-hover-down" data-toggle="modal" data-target="#modal1">Create User</button><button class="btn btn-light shadow-hover-down"
                                    type="submit">Log in</button></div>
                        </div>


                    </form>
                </div>
            </div>
        </div>
    </main>
    <script src="/bootstrap/4/js/popper.min.js"></script>
    <script src="/bootstrap/4/js/bootstrap.min.js"></script>
